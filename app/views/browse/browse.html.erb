<ul id="slides">
    <% @users.each do |user| %>
    <div class="slide" data-id="<%= user.id %>">
        <li><%= user.first_name %></li>
    </div>
    <% end %>
</ul>

<div id="slide-controls">
    <span id="decline"><i class="fas fa-check fa-2x"></i></span>
    <span id="approve"><i class="fas fa-heart fa-2x"></i></span>
</div>